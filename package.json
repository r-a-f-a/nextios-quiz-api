{
  "name": "nextios-quiz-api",
  "version": "1.0.0",
  "description": "API for Nextios Quiz Game",
  "main": "src/index.ts",
  "author": "Michel Lima",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "npm i && npm run build && node dist/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test": "yarn test:unit && yarn test:functional",
    "test:unit": "jest",
    "test:functional": "jest --projects ./test --runInBand"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.36",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.6.0",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "jest": "^26.4.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-node-dev": "^1.0.0-pre.58"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.4",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "generate-password": "^1.5.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.10.2",
    "nodemailer": "^6.4.11",
    "typescript": "^4.0.3"
  },
  "_moduleAlias": {
    "@src": "./src",
    "@test": "./test"
  }
}
